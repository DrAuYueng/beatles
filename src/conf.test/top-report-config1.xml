                 <entry name="服务器IP" key="apiName,localIp" value="plain($localIp$)" condition="$apiName$=taobao.item.vip.update"/> 
                 <entry name="服务名称" key="apiName,localIp" value="plain($apiName$)" condition="$apiName$=taobao.item.vip.update"/> 
                 <entry name="访问成功次数" key="apiName,localIp" value="count()" condition="$errorCode$=0&amp;$apiName$=taobao.item.vip.update"/>
