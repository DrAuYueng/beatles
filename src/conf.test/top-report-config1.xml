                 <entry name="成功访问平均消耗时间" key="apiName,localIp" value="average($transactionConsumeTime$)" condition="$errorCode$=0&amp;$apiName$=taobao.item.vip.update"/>
                 <entry name="成功访问平均业务消耗时间" key="apiName,localIp" value="average($serviceConsumeTime$)" condition="$errorCode$=0&amp;$apiName$=taobao.item.vip.update"/>
