                    <entry name="占总量比例" key="localIp" value="plain(entry(17)/entry(sum:17))" valuefilter="&gt;=0&amp;isnumber&amp;round:3"/>
                    <entry id="18"/>
                    <entry name="成功率" key="localIp" value="plain(entry(18)/entry(machineTotalCount))" valuefilter="&gt;=0&amp;isnumber&amp;round:3"/>
